{
    "name": "student-progress-monitor",
    "packageManager": "yarn@4.1.1",
    "private": true,
    "author": "angelique",
    "main": "src/main.ts",
    "scripts": {
        "start": "yarn ts-node .",
        "db": "yarn mikro-orm",
        "db:create": "yarn mikro-orm schema:create --run",
        "db:migrate:create": "yarn mikro-orm migration:create",
        "db:migrate": "yarn mikro-orm migration:up",
        "db:seed": "yarn mikro-orm seeder:run",
        "db:fresh": "yarn mikro-orm migration:fresh",
        "db:fresh:seed": "yarn mikro-orm migration:fresh --seed",
        "integration-test:start": "NODE_ENV=integration-test yarn ts-node .",
        "integration-test:db:seed": "NODE_ENV=integration-test yarn mikro-orm seeder:run -c IntegrationTestDatabaseSeeder",
        "integration-test:db:fresh": "NODE_ENV=integration-test yarn mikro-orm migration:fresh",
        "integration-test:db:fresh:seed": "NODE_ENV=integration-test yarn mikro-orm migration:fresh --seed IntegrationTestDatabaseSeeder",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "dependencies": {
        "@apollosoftwarexyz/cinnamon": "^0.1.4",
        "@apollosoftwarexyz/cinnamon-database": "^0.1.4",
        "@apollosoftwarexyz/cinnamon-plugin-asl-protocol": "^2.0.1",
        "@apollosoftwarexyz/cinnamon-validator": "^0.1.4",
        "@mikro-orm/cli": "5.9.4",
        "@mikro-orm/core": "5.9.4",
        "@mikro-orm/migrations": "5.9.4",
        "@mikro-orm/postgresql": "5.9.4",
        "@types/node": "^20.5.7",
        "@yarnpkg/sdks": "^3.0.0-rc.50",
        "argon2": "^0.31.0",
        "reflect-metadata": "^0.1.13",
        "toml": "^3.0.0",
        "ts-node": "^10.8.1",
        "typescript": "^5.2.2"
    },
    "devDependencies": {
        "@mikro-orm/seeder": "5.9.4",
        "@types/mocha": "^10.0.6",
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0",
        "eslint": "^8.54.0",
        "eslint-plugin-import-quotes": "^0.0.1",
        "mocha": "^10.4.0",
        "nyc": "^15.1.0",
        "ts-mocha": "^10.0.0"
    },
    "mikro-orm": {
        "useTsNode": true,
        "configPaths": [
            "./src/mikro-orm.config.ts"
        ]
    }
}
