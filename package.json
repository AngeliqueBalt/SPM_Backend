{
    "name": "student-progress-monitor",
    "packageManager": "yarn@4.0.2",
    "private": true,
    "author": "angelique",
    "main": "src/main.ts",
    "scripts": {
        "start": "yarn ts-node .",
        "db": "yarn mikro-orm",
        "db:create": "yarn mikro-orm schema:create --run",
        "db:migrate:create": "yarn mikro-orm migration:create",
        "db:migrate": "yarn mikro-orm migration:up",
        "lint": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "dependencies": {
        "@apollosoftwarexyz/cinnamon": "^0.1.4",
        "@apollosoftwarexyz/cinnamon-database": "^0.1.4",
        "@apollosoftwarexyz/cinnamon-plugin-asl-protocol": "^2.0.1",
        "@apollosoftwarexyz/cinnamon-validator": "^0.1.4",
        "@mikro-orm/cli": "^5.7.14",
        "@mikro-orm/core": "^5.7.14",
        "@mikro-orm/migrations": "^5.7.14",
        "@mikro-orm/postgresql": "^5.7.14",
        "@types/node": "^20.5.7",
        "@yarnpkg/sdks": "^3.0.0-rc.50",
        "argon2": "^0.31.0",
        "reflect-metadata": "^0.1.13",
        "ts-node": "^10.8.1",
        "typescript": "^5.2.2"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.11.0",
        "@typescript-eslint/parser": "^6.11.0",
        "eslint": "^8.54.0",
        "eslint-plugin-import-quotes": "^0.0.1"
    },
    "mikro-orm": {
        "useTsNode": true,
        "configPaths": [
            "./src/mikro-orm.config.ts"
        ]
    }
}
